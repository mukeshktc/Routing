<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Popover showHeader="false" contentWidth="300px" contentHeight="300px" placement="Bottom">
		<NavContainer id="navCon">
			<Page title="Select Price Range">
				<List id="list" items="{/SelectionCriteria}">
					<StandardListItem title="{filter}" type="Active" press="onFilter"></StandardListItem>
				</List>
			</Page>
		</NavContainer>
	</Popover>
</core:FragmentDefinition>
